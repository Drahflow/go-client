      },
      {
        "expectedValue": "FallthroughValue",
        "expectError": false,
        "user": {
          "key": "excluded",
          "email": "user@EndsWithA.com",
          "custom": {
            "group": "GroupA",
            "exclude": "yes"
          }
        }
      },
      {
        "expectedValue": "FallthroughValue",
        "expectError": false,
        "user": {
          "key": "inNone",
          "email": "user@notIncluded.com",
          "custom": {
            "group": "GroupNotInTheClauses"
          }
        }
      },
      {
        "expectedValue": "DefaultValue",
        "expectError": true,
        "user": {
          "key": null,
          "email": "user@EndsWithA.com",
          "custom": {
            "group": "GroupB"
          }
        }
